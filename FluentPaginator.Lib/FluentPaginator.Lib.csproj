<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <Authors>Ola-Jed</Authors>
        <Nullable>enable</Nullable>
        <Title>A Fluent Paginator for .net</Title>
        <Description>A Package for IQueryable/IEnumerable Pagination</Description>
        <PackageProjectUrl>https://github.com/Ola-jed/FluentPaginator</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/Ola-jed/FluentPaginator/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/Ola-jed/FluentPaginator</RepositoryUrl>
        <AssemblyVersion>1.0</AssemblyVersion>
        <Version>1.0</Version>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
        <DocumentationFile>doc\FluentPaginator.Lib.xml</DocumentationFile>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
        <DocumentationFile>doc\FluentPaginator.Lib.xml</DocumentationFile>
    </PropertyGroup>

</Project>
